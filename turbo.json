{
	"$schema": "./node_modules/turbo/schema.json",
	"ui": "tui",
	"tasks": {
		"topo": {
			"dependsOn": ["^topo"]
		},
		"build": {
			"dependsOn": ["^build"]
		},
		"migrate": {
			"dependsOn": ["^topo", "^build"],
			"cache": false
		},
		"seed": {
			"dependsOn": ["^topo", "^build"],
			"cache": false
		},
		"lint": {
			"dependsOn": ["^topo", "^build"]
		},
		"typecheck": {
			"dependsOn": ["^topo", "^build"]
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"format": {
			"outputLogs": "new-only"
		},
		"//#format:root": {
			"outputLogs": "new-only"
		}
	}
}
