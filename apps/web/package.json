		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.15",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@remote-cache/api": "workspace:*",
		"@remote-cache/core": "workspace:*",
		"@remote-cache/db": "workspace:*",
		"@remote-cache/storage": "workspace:*",
		"@sentry/tanstackstart-react": "^10.15.0",
		"@t3-oss/env-core": "^0.13.8",
		"@tanstack/react-form": "^1.23.4",
		"@tanstack/react-query": "^5.90.2",
		"@tanstack/react-query-devtools": "^5.90.2",
		"@tanstack/react-router": "^1.132.7",
		"@tanstack/react-router-devtools": "^1.132.7",
		"@tanstack/react-router-ssr-query": "^1.132.7",
		"@tanstack/react-start": "^1.132.9",
		"better-auth": "^1.3.18",
		"chalk": "^5.6.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.544.0",
		"react": "^19.1.1",
		"react-day-picker": "^9.11.0",
		"react-dom": "^19.1.1",
		"react-lazily": "^0.9.2",
		"remeda": "^2.32.0",
		"sonner": "^2.0.7",
		"std-env": "^3.9.0",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.1.11"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@netlify/vite-plugin": "^2.6.0",
		"@remote-cache/eslint-config": "workspace:*",
		"@remote-cache/prettier-config": "workspace:*",
		"@remote-cache/tsconfig": "workspace:*",
		"@sentry/vite-plugin": "^4.3.0",
		"@tailwindcss/vite": "^4.1.13",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
