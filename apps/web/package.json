{
	"name": "@remote-cache/web",
	"description": "The web interface for the remote cache",
	"keywords": [
		"cache",
		"remote",
		"web"
	],
	"author": "Peter van Meijgaard <peter@vivid-web.com>",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"auth:generate": "dotenv -- pnpx @better-auth/cli generate --config ./src/lib/auth.server.ts --output ../../packages/db/src/auth-schema.ts",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"migrate": "dotenv -- tsx ./scripts/migrate.ts",
		"seed": "dotenv -- txs ./scripts/seed.ts",
		"lint": "eslint --cache --cache-location .cache/eslint .",
		"typecheck": "tsc --noEmit",
		"format": "prettier --cache --cache-location .cache/prettier --check ."
	},
	"dependencies": {
		"@netlify/blobs": "^10.0.11",
		"@paralleldrive/cuid2": "^2.2.2",
		"@radix-ui/react-alert-dialog": "^1.1.15",
