		"prettier": "^3.6.2",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"prettier": "@remote-cache/prettier-config"
}
