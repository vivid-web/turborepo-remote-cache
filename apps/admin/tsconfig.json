{
	"compilerOptions": {
		"jsx": "react-jsx",
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ES2022",
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true,
		"strictNullChecks": true,
		"baseUrl": ".",
		"types": ["vite/client"],
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
