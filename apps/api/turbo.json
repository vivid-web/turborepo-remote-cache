{
	"$schema": "../../node_modules/turbo/schema.json",
	"extends": ["//"],
	"tasks": {
		"build": {
			"outputs": ["dist", ".cache/tsconfig.build.tsbuildinfo"]
		},
		"lint": {
			"dependsOn": ["^topo", "build"],
			"outputs": [".cache/eslint"]
		},
		"typecheck": {
			"dependsOn": ["^topo", "build"],
			"outputs": [".cache/tsconfig.tsbuildinfo"]
		},
		"format": {
			"outputs": [".cache/prettier"]
		}
	}
}
