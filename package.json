{
	"name": "turborepo-remote-cache",
	"description": "The admin panel for Turborepo cache",
	"keywords": [
		"Turborepo",
		"cache",
		"remote",
		"admin"
	],
	"author": "Peter van Meijgaard <peter@vivid-web.com>",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "run-p dev:app db:studio",
		"dev:app": "vite dev",
		"build": "vite build",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "dotenv -e .env -- tsx scripts/migrate.mts",
		"db:seed": "dotenv -e .env -- tsx scripts/seed.mts",
		"db:studio": "drizzle-kit studio",
		"lint": "eslint --cache .",
		"format": "prettier --check --cache .",
		"prepare": "husky"
	},
	"dependencies": {
		"@neondatabase/serverless": "^1.0.1",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@tanstack/react-query": "^5.80.10",
		"@tanstack/react-query-devtools": "^5.80.10",
		"@tanstack/react-router": "^1.121.27",
		"@tanstack/react-router-devtools": "^1.121.27",
		"@tanstack/react-router-with-query": "^1.121.27",
		"@tanstack/react-start": "^1.121.32",
		"better-auth": "^1.2.10",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.2",
		"lucide-react": "^0.522.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint/compat": "^1.3.0",
		"@eslint/js": "^9.29.0",
		"@tailwindcss/vite": "^4.1.10",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"dotenv-cli": "^8.0.0",
		"drizzle-kit": "^0.31.1",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-import-resolver-typescript": "^4.4.3",
		"eslint-plugin-import-x": "^4.15.2",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"tailwindcss": "^4.1.10",
		"tsx": "^4.20.3",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1",
		"vite": "^6.3.5",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"engines": {
		"node": ">=22.16.0"
	},
	"packageManager": "pnpm@10.12.1"
}
