{
	"name": "turborepo-remote-cache",
	"description": "The monorepo for the Turborepo remote cache project",
	"keywords": [
		"Turborepo",
		"cache",
		"remote",
		"admin"
	],
	"author": "Peter van Meijgaard <peter@vivid-web.com>",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "pnpm --filter=\"@turborepo-remote-cache/*\" run dev",
		"build": "pnpm --filter=\"@turborepo-remote-cache/*\" run build",
		"lint": "pnpm --filter=\"@turborepo-remote-cache/*\" run lint",
		"format": "prettier --check --cache .",
		"prepare": "husky"
	},
	"devDependencies": {
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14"
	},
	"engines": {
		"node": ">=22.19.0"
	},
	"packageManager": "pnpm@10.15.0"
}
