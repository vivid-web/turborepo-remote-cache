{
	"name": "@remote-cache/db",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"default": "./dist/client.js"
		},
		"./schema": {
			"types": "./dist/schema.d.ts",
			"default": "./dist/schema.js"
		}
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"dev": "tsc --project tsconfig.build.json --watch",
		"lint": "eslint --cache --cache-location .cache/eslint .",
		"typecheck": "tsc --noEmit",
		"format": "prettier --cache --cache-location .cache/prettier --check ."
	},
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"@t3-oss/env-core": "^0.13.8",
		"drizzle-orm": "^0.44.6",
		"pg": "^8.16.3",
		"std-env": "^3.9.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@remote-cache/eslint-config": "workspace:*",
		"@remote-cache/prettier-config": "workspace:*",
		"@remote-cache/tsconfig": "workspace:*",
		"@types/pg": "^8.15.5",
		"eslint": "^9.37.0",
		"prettier": "^3.6.2",
		"typescript": "^5.9.3"
	},
	"prettier": "@remote-cache/prettier-config"
}
