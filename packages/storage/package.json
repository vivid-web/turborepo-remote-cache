{
	"name": "@remote-cache/storage",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./local": {
			"types": "./dist/local/index.d.ts",
			"default": "./dist/local/index.js"
		},
		"./netlify-blobs": {
			"types": "./dist/netlify-blobs/index.d.ts",
			"default": "./dist/netlify-blobs/index.js"
		}
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"dev": "tsc --project tsconfig.build.json --watch",
		"lint": "eslint --cache --cache-location .cache/eslint .",
		"typecheck": "tsc --noEmit",
		"format": "prettier --cache --cache-location .cache/prettier --check ."
	},
	"peerDependencies": {
		"@netlify/blobs": "^10.0.10"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@hono/node-server": "^1.19.5",
		"@netlify/blobs": "^10.1.0",
		"@remote-cache/eslint-config": "workspace:*",
		"@remote-cache/prettier-config": "workspace:*",
		"@remote-cache/tsconfig": "workspace:*",
		"@types/node": "^22.18.10",
		"eslint": "catalog:",
		"prettier": "catalog:",
		"typescript": "catalog:"
	},
	"prettier": "@remote-cache/prettier-config"
}
