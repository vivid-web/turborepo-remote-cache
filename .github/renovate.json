{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"configMigration": true,
	"extends": [
		"config:recommended",
		"group:allNonMajor",
		"schedule:weekly",
		":approveMajorUpdates",
		":automergeMinor",
		":disablePeerDependencies",
		":maintainLockFilesMonthly",
		":semanticCommits"
	],
	"ignorePresets": [":ignoreModulesAndTests"],
	"labels": ["Dependencies"],
	"rangeStrategy": "bump",
	"postUpdateOptions": ["pnpmDedupe"]
}
